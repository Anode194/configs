!urxvt settings
URxvt.font: xft:inconsolata:size=10
URxvt.scrollBar: false
urxvt.transparent: false
urxvt.shading: 30
urxvt.scrollWithBuffer: true
urxvt.scrollTtyKeypress: true



!rofi settings
rofi.theme: Pop-Dark
rofi.lines: 8
rofi.width: 30
rofi.hidescrollbar: true
rofi.color-normal: #ffffff
rofi.color-window: #ABA5DC
rofi.color-urgent: #ABA5DC
rofi.color-active: #ABA5DC
rofi.padding: 20
rofi.font: hack 12
!resize font settings.
URxvt.perl-lib: ~/.urxvt/ext/
urxvt.perl-ext-common: default,tabbed,matcher,resize-font,-tabbed
URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show
